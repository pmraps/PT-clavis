msgctxt
msgid "`File' Menu"
msgstr ""
"MENU FICHEIRO - as cadeias seguintes são usadas no menu \"Ficheiro\" de "
"qualquer programa"

# en -
# pt -
# Shortcut / Atalho: F
msgctxt
msgid "_File"
msgstr "_Ficheiro"

# en - Can be 'New <anything>'
# pt - Pode ser "Novo(a) <qualquer coisa>"
# Shortcut / Atalho: N
msgctxt
msgid "_New"
msgstr "_Novo(a)"

# en - Can be 'Open <anything>'
# pt - Pode ser "Abrir <qualquer coisa>"
# Caso em que o atalho é forçosamente diferente do original. A primeira letra é a opção favorita
# Shortcut / Atalho: A
msgctxt
msgid "_Open"
msgstr "_Abrir"

# en -
# pt -
# Shortcut / Atalho: D
msgctxt
msgid "_Recent documents"
msgstr "Documentos _recentes"

# en - Save Data, Store clothing, Save lives
# pt - Gravar dados, Guardar roupa, Salvar vidas
# Shortcut / Atalho: G
msgctxt
msgid "_Save"
msgstr "_Gravar"

# en - cannot be original A (used for 'Open') nor G (used for 'Save').
# Chosen V as the least probable under 'File' menu.
# pt -não pode ser o original A (já usado em "Abrir") nem G (já usado em "Gravar").
# Escolhido "V" como menos provável no menu "Ficheiro".
# Shortcut / Atalho: V
msgctxt
msgid "S_ave as..."
msgstr "Gra_var como..."

# en -
# pt -
# Shortcut / Atalho: C
msgctxt
msgid "Save a _copy"
msgstr "Gravar uma _cópia"

# en -
# pt - A utilização de "tudo" evita o género masculino/feminino
# Shortcut / Atalho: T
msgctxt
msgid "Save all"
msgstr "Gravar _tudo"

# en - Export, Export as, etc.
# pt - Exportar, Exportar como, etc.
# Shortcut / Atalho: E
msgctxt
msgid "_Export"
msgstr "_Exportar"

# en - Import
# pt - Importar
# Shortcut / Atalho: I
msgctxt
msgid "_Import"
msgstr "_Importar"

# en -
# pt -
# Shortcut / Atalho: P
msgctxt
msgid "_Print"
msgstr "Im_primir"

# en -
# pt - Preferi Antevisão por ser mais curta. É usada em botões onde o
# espaço é limitado
# Shortcut / Atalho: E
msgctxt
msgid "Print Pr_eview"
msgstr "Ant_ever impressão"

# en -
# pt - É preferível impressão a impressora.
# Shortcut / Atalho: R
msgctxt
msgid "P_rinter Setup"
msgstr "Configurar imp_ressão"

# en -
# pt -
# Shortcut / Atalho: G
msgctxt
msgid "Pa_ge Setup"
msgstr "Configurar pá_gina"

# en -
# pt -
# Shortcut / Atalho: S
msgctxt
msgid "_Exit"
msgstr "_Sair"

# en -
# pt -
# Shortcut / Atalho: S
msgctxt
msgid "_Quit"
msgstr "_Sair"

